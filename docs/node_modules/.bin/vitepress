#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shenkun/WorkSpace/Github/whale/node_modules/.pnpm/vitepress@1.2.2_@algolia+client-search@4.23.3_@types+node@18.7.8_async-validator@4.2.5_axios@_muj7rrdxjaplr6whu5fnvtz37q/node_modules/vitepress/bin/node_modules:/Users/shenkun/WorkSpace/Github/whale/node_modules/.pnpm/vitepress@1.2.2_@algolia+client-search@4.23.3_@types+node@18.7.8_async-validator@4.2.5_axios@_muj7rrdxjaplr6whu5fnvtz37q/node_modules/vitepress/node_modules:/Users/shenkun/WorkSpace/Github/whale/node_modules/.pnpm/vitepress@1.2.2_@algolia+client-search@4.23.3_@types+node@18.7.8_async-validator@4.2.5_axios@_muj7rrdxjaplr6whu5fnvtz37q/node_modules:/Users/shenkun/WorkSpace/Github/whale/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shenkun/WorkSpace/Github/whale/node_modules/.pnpm/vitepress@1.2.2_@algolia+client-search@4.23.3_@types+node@18.7.8_async-validator@4.2.5_axios@_muj7rrdxjaplr6whu5fnvtz37q/node_modules/vitepress/bin/node_modules:/Users/shenkun/WorkSpace/Github/whale/node_modules/.pnpm/vitepress@1.2.2_@algolia+client-search@4.23.3_@types+node@18.7.8_async-validator@4.2.5_axios@_muj7rrdxjaplr6whu5fnvtz37q/node_modules/vitepress/node_modules:/Users/shenkun/WorkSpace/Github/whale/node_modules/.pnpm/vitepress@1.2.2_@algolia+client-search@4.23.3_@types+node@18.7.8_async-validator@4.2.5_axios@_muj7rrdxjaplr6whu5fnvtz37q/node_modules:/Users/shenkun/WorkSpace/Github/whale/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
